# vim: set sts=2 ts=8 sw=2 tw=99 et ft=python:
import os.path

binary = builder.compiler.Library('loader')
binary.sources += [ 'loader.cpp' ]
binary.compiler.cxxincludes += [
  os.path.join(HLTVSupport.mms_root, 'core'),
  os.path.join(HLTVSupport.sm_root, 'public', 'amtl', 'amtl'),
  os.path.join(HLTVSupport.sm_root, 'public', 'amtl'),
]

HLTVSupport.binaries += [ builder.Add(binary) ]
